{
  "name": "znui-react",
  "version": "1.2.1",
  "description": "znui react is the front-end basic framework.",
  "main": "production.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/znui/znui-react.git"
  },
  "scripts": {
    "webpack": "rimraf dist && webpack",
    "webpack-watch": "rimraf dist && webpack --watch",
    "clean-dist": "rimraf dist",
    "clean-build": "rimraf build",
    "release": "npm run clean-dist && npm run clean-build && babel src -d build && webpack --uglify",
    "build": "rimraf build && babel src -d build",
    "dist": "rimraf dist && webpack --uglify",
    "publish": "npm publish",
    "unpublish": "npm unpublish"
  },
  "keywords": [
    "front-end",
    "react",
    "znui"
  ],
  "author": "yangyxu",
  "email": "xuyy0618@qq.com",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "create-react-class": "^15.6.3",
    "file-loader": "^4.2.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "url-loader": "^2.1.0",
    "znui": "^1.1.7"
  }
}
