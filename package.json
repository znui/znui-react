{
  "name": "znui-react",
  "version": "2.1.2",
  "description": "znui react is the front-end basic framework.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/znui/znui-react.git"
  },
  "scripts": {
    "webpack": "rimraf dist && webpack",
    "webpack-watch": "rimraf dist && webpack --watch",
    "clean-dist": "rimraf dist",
    "clean-build": "rimraf build",
    "release": "npm run clean-dist && npm run clean-build && babel src -d build && webpack --uglify",
    "build": "rimraf build && babel src -d build",
    "build-watch": "rimraf build && babel src -d build --watch",
    "dist": "rimraf dist && webpack --uglify",
    "publish": "npm publish",
    "unpublish": "npm unpublish"
  },
  "keywords": [
    "front-end",
    "react",
    "znui"
  ],
  "author": "yangyxu",
  "email": "xuyy0618@qq.com",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "create-file-webpack": "^1.0.2",
    "create-react-class": "^15.6.3",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "raw-loader": "^0.5.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.2",
    "scss": "^0.2.4",
    "scss-loader": "0.0.1",
    "strip-loader": "^0.1.2",
    "style-loader": "^1.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "znui": "latest"
  }
